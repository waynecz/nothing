{
  "apps": [{
    "name": "wayne",
    "script": "bin/www",
    "instances": 1,
    "max_memory_restart": "512M",
    "autorestart": true,
    "env": {
      "NODE_ENV": "dev",
      "MOCK": "yes"
    },
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "error_file": "log/err.log",
    "out_file": "log/logs.log"
  }]
}